<?xml version="1.0" encoding="UTF-8"?>
<workbook>
	<worksheet name="Sheet1">
		<section startRow="0" endRow="0" />
		<loop startRow="1" endRow="1" items="userList" var="user"  
			varType="com.isoftstone.tyw.entity.auths.User">
			<section startRow="1" endRow="1">
				<mapping row="1" col="0" >user.id</mapping>
				<mapping row="1" col="1" >user.username</mapping>
				<mapping row="1" col="2" >user.password</mapping>
				<mapping row="1" col="3">user.name</mapping>
				<mapping row="1" col="4">user.addr</mapping>
				<mapping row="1" col="5">user.linkman</mapping>
				<mapping row="1" col="6">user.fphone</mapping>
				<mapping row="1" col="7">user.fax</mapping>
				<mapping row="1" col="8">user.zip</mapping>
				<mapping row="1" col="9">user.email</mapping>
				
			</section>
			<loopbreakcondition>
				<rowcheck offset="0">
					<cellcheck offset="0">END</cellcheck>
				</rowcheck>
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook>