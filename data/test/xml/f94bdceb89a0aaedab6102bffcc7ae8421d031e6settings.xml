<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <profiles>
        <profile>
            <id>alwaysActiveProfile</id>
            <repositories>
                <repository>
                    <id>central</id>
                    <name>Maven Default Release Repository</name>
                    <url>http://repo1.maven.org/maven2/</url>
                </repository>
                <repository>
                    <id>springsource-external-repository</id>
                    <name>SpringSource External Lib Release Repository</name>
                    <url>http://repository.springsource.com/maven/bundles/external</url>
                </repository>
                <repository>
                    <id>zodiac-repository</id>
                    <name>Zodiac Release Repository</name>
                    <url>http://zodiac.springsource.com/maven/bundles/release</url>
                </repository>
                <repository>
                    <id>com.springsource.repository.bundles.release</id>
                    <name>SpringSource Enterprise Bundle Repository - SpringSource Bundle Releases</name>
                    <url>http://repository.springsource.com/maven/bundles/release</url>
                </repository>
                <repository>
                    <id>com.springsource.repository.bundles.external</id>
                    <name>SpringSource Enterprise Bundle Repository - External Bundle Releases</name>
                    <url>http://repository.springsource.com/maven/bundles/external</url>
                </repository>
                <repository>
                    <id>xdir-repository</id>
                    <name>mrduguo's Maven Release Repository</name>
                    <url>http://mrduguo.github.com/maven/repos/release</url>
                </repository>
                    <repository>
                        <id>com.asual.maven.public</id>
                        <name>Asual Public Repository for less-engine</name>
                        <url>http://www.asual.com/maven/content/groups/public</url>
                    </repository>
            </repositories>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>alwaysActiveProfile</activeProfile>
    </activeProfiles>
</settings>
