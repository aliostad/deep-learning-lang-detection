<?xml version="1.0" encoding="UTF-8"?>
<workbook>
	<worksheet name="Sheet1">
		<section startRow="0" endRow="0" />
		<loop startRow="1" endRow="1" items="formList" var="form"  
			varType="com.isoftstone.tyw.entity.info.Form">
			<section startRow="1" endRow="1">
				<mapping row="1" col="0">form.id</mapping>
				<mapping row="1" col="1">form.title</mapping>
				<mapping row="1" col="2">form.shortTitle</mapping>>
				<mapping row="1" col="3">form.source</mapping>
				<mapping row="1" col="4">form.personnelTag</mapping>
				<mapping row="1" col="5">form.areaTag</mapping>
				<mapping row="1" col="6">form.knowledgeTag</mapping>
				<mapping row="1" col="7">form.subitemTag</mapping>
				<mapping row="1" col="8">form.begincreateDate</mapping>
			</section>
			<loopbreakcondition>
				<rowcheck offset="0">
					<cellcheck offset="0">END</cellcheck>
				</rowcheck>
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook>