#!/bin/sh
# This script was created by make-mzscheme-launcher

# {{{ bindir
# Make this PATH-independent
saveP="$PATH"
PATH="/usr/bin:/bin"

# Remember current directory
saveD=`pwd`

# Find absolute path to this script,
# resolving symbolic references to the end
# (changes the current directory):
D=`dirname "$0"`
F=`basename "$0"`
cd "$D"
while test -L "$F"; do
  P=`readlink "$F"`
  D=`dirname "$P"`
  F=`basename "$P"`
  cd "$D"
done
D=`pwd`

# Restore current directory
cd "$saveD"

bindir="$D"
PATH="$saveP"
# }}} bindir

exec "${bindir}/racket" -N "$0" '-I' 'scheme/init' '-l-' 'preprocessor/mzpp-run.ss'  ${1+"$@"}
