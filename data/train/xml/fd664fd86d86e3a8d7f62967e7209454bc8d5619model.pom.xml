<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.bluexml.side.dependencies</groupId>
	<artifactId>tmpProject</artifactId>
	<packaging>jar</packaging>
	<version>1.0.0</version>
	<name>tmpProject</name>
	<url>http://www.bluexml.com</url>
	<dependencies></dependencies>
	<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<artifactId>maven-dependency-plugin</artifactId>
					<version>2.2</version>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>
	<profiles>
		<profile>
			<id>public</id>
			<activation>
				<activeByDefault>false</activeByDefault>
			</activation>
			<repositories>
				<repository>
					<id>public</id>
					<url>http://pipin.bluexml.com/nexus/content/groups/public
					</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>
				<repository>
					<id>public-snapshots</id>
					<url>
						http://pipin.bluexml.com/nexus/content/groups/public-snapshots
					</url>
					<releases>
						<enabled>false</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<id>public-snapshots</id>
					<url>
						http://pipin.bluexml.com/nexus/content/groups/public-snapshots
					</url>
				</pluginRepository>
				<pluginRepository>
					<id>public</id>
					<url>http://pipin.bluexml.com/nexus/content/groups/public
					</url>
				</pluginRepository>
			</pluginRepositories>
			<distributionManagement>
				<repository>
					<id>side-releases</id>
					<url>http://pipin.bluexml.com/nexus/content/repositories/side-releases
					</url>
				</repository>
				<snapshotRepository>
					<id>side-snapshots</id>
					<url>http://pipin.bluexml.com/nexus/content/repositories/side-snapshots
					</url>
				</snapshotRepository>
			</distributionManagement>
		</profile>
		<profile>
			<id>offline</id>
			<activation>
				<activeByDefault>false</activeByDefault>
			</activation>
			<repositories>
				<repository>
					<id>central</id>
					<url>${settings.localRepository}
					</url>
				</repository>
				<repository>
					<id>side-releases</id>
					<url>${settings.localRepository}
					</url>
				</repository>
				<repository>
					<id>side-snapshots</id>
					<url>${settings.localRepository}
					</url>
				</repository>
				<repository>
					<id>public-snapshots</id>
					<url>${settings.localRepository}
					</url>
				</repository>
				<repository>
					<id>public</id>
					<url>${settings.localRepository}
					</url>
				</repository>
				<repository>
					<id>BXKB-repo</id>
					<url>${settings.localRepository}
					</url>
				</repository>
				<repository>
					<id>BXKB-repo-snapshots</id>
					<url>${settings.localRepository}
					</url>
				</repository>
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<id>central</id>
					<url>${settings.localRepository}
					</url>
				</pluginRepository>
				<pluginRepository>
					<id>side-releases</id>
					<url>${settings.localRepository}
					</url>
				</pluginRepository>
				<pluginRepository>
					<id>side-snapshots</id>
					<url>${settings.localRepository}
					</url>
				</pluginRepository>
				<pluginRepository>
					<id>public</id>
					<url>${settings.localRepository}
					</url>
				</pluginRepository>
				<pluginRepository>
					<id>public-snapshots</id>
					<url>${settings.localRepository}
					</url>
				</pluginRepository>
				<pluginRepository>
					<id>BXKB-repo</id>
					<url>${settings.localRepository}
					</url>
				</pluginRepository>
				<pluginRepository>
					<id>BXKB-repo-snapshots</id>
					<url>${settings.localRepository}
					</url>
				</pluginRepository>
			</pluginRepositories>
		</profile>
	</profiles>
</project>

