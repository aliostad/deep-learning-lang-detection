<?xml version="1.0" encoding="US-ASCII"?>
<configuration xmlns="http://fedorahosted.org/xmvn/CONFIG/1.0.0">
  <resolverSettings>
    <metadataRepositories>
      <repository>${build.directory}/root/etc/xdg/xmvn/depmap.xml</repository>
    </metadataRepositories>
    <prefixes>
      <prefix>${project.build.directory}/root</prefix>
    </prefixes>
    <blacklist>
      <artifact>
        <groupId>javax.activation</groupId>
        <artifactId>activation</artifactId>
      </artifact>
      <artifact>
        <groupId>org.eclipse.jetty.orbit</groupId>
        <artifactId>javax.activation</artifactId>
      </artifact>
      <artifact>
        <groupId>org.apache.maven.wagon</groupId>
        <artifactId>wagon-webdav</artifactId>
      </artifact>
      <artifact>
        <groupId>org.apache.maven.wagon</groupId>
        <artifactId>wagon-webdav-jackrabbit</artifactId>
      </artifact>
    </blacklist>
  </resolverSettings>
  <installerSettings>
    <metadataDir>usr/share/maven-fragments</metadataDir>
  </installerSettings>
  <repositories>
    <repository>
      <id>base-raw-pom</id>
      <type>flat</type>
      <properties>
        <root>usr/share/maven-poms</root>
      </properties>
      <stereotypes>
        <stereotype>
          <extension>pom</extension>
        </stereotype>
      </stereotypes>
    </repository>
    <repository>
      <id>base-effective-pom</id>
      <type>flat</type>
      <properties>
        <root>usr/share/maven-effective-poms</root>
      </properties>
      <stereotypes>
        <stereotype>
          <extension>pom</extension>
        </stereotype>
      </stereotypes>
    </repository>
    <repository>
      <id>base-jar</id>
      <type>jpp</type>
      <properties>
        <root>usr/share/java</root>
      </properties>
      <stereotypes>
        <stereotype>
          <extension>jar</extension>
        </stereotype>
      </stereotypes>
    </repository>
    <repository>
      <id>base-jni</id>
      <type>jpp</type>
      <properties>
        <root>usr/lib/java</root>
      </properties>
      <stereotypes>
        <stereotype>
          <extension>jar</extension>
        </stereotype>
      </stereotypes>
    </repository>
    <repository>
      <id>mojo-signatures</id>
      <type>jpp</type>
      <properties>
        <root>usr/share/mojo-signatures</root>
      </properties>
      <stereotypes>
        <stereotype>
          <extension>signature</extension>
        </stereotype>
      </stereotypes>
    </repository>
    <repository>
      <id>base-resolve</id>
      <type>compound</type>
      <configuration>
        <repositories>
          <repository>base-effective-pom</repository>
          <repository>base-raw-pom</repository>
          <repository>base-jar</repository>
          <repository>base-jni</repository>
          <repository>mojo-signatures</repository>
        </repositories>
      </configuration>
    </repository>
    <repository>
      <id>base-install</id>
      <type>compound</type>
      <configuration>
        <repositories>
          <repository>base-raw-pom</repository>
          <repository>base-jar</repository>
          <repository>base-jni</repository>
        </repositories>
      </configuration>
    </repository>
    <repository>
      <id>resolve</id>
      <type>compound</type>
      <properties>
        <prefix>${project.build.directory}/root</prefix>
      </properties>
      <configuration>
        <repositories>
          <repository>base-resolve</repository>
        </repositories>
      </configuration>
    </repository>
    <repository>
      <id>install</id>
      <type>compound</type>
      <configuration>
        <repositories>
          <repository>base-install</repository>
        </repositories>
      </configuration>
    </repository>
    <repository>
      <id>install-raw-pom</id>
      <type>compound</type>
      <configuration>
        <repositories>
          <repository>base-raw-pom</repository>
        </repositories>
      </configuration>
    </repository>
    <repository>
      <id>install-effective-pom</id>
      <type>compound</type>
      <configuration>
        <repositories>
          <repository>base-effective-pom</repository>
        </repositories>
      </configuration>
    </repository>
  </repositories>
</configuration>
