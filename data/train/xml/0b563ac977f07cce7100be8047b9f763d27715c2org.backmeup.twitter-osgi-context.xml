<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"	
	default-activation="lazy">
	<service id="twitterDescriptorService" ref="twitterDescriptor" auto-export="interfaces">
		<service-properties>
			<entry key="name" value="org.backmeup.twitter"/>
		</service-properties>
	</service>
	<service id="twitterAuthService" ref="twitterAuthenticator" auto-export="interfaces">
		<service-properties>
			<entry key="name" value="org.backmeup.twitter"/>
		</service-properties>
	</service>
	<service id="twitterDatasourceService" ref="twitterDatasource" auto-export="interfaces">
		<service-properties>
			<entry key="name" value="org.backmeup.twitter"/>
		</service-properties>
	</service>
	<service id="twitterValidatorService" ref="twitterValidator" auto-export="interfaces">
		<service-properties>
			<entry key="name" value="org.backmeup.twitter"/>
		</service-properties>
	</service>
</blueprint>