<?xml version="1.0"?>
<object name="portal_actions" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <object name="object_buttons" meta_type="CMF Action Category">
    <object name="duplicate" meta_type="CMF Action" i18n:domain="collective.duplicateaction">
      <property name="title" i18n:translate="">Duplicate</property>
      <property name="description" i18n:translate=""></property>
      <property name="url_expr">string:$object_url/@@object-duplicate</property>
      <property name="link_target"></property>
      <property name="icon_expr"></property>
      <property
        name="available_expr">object/@@duplicate-allowed|nothing</property>
      <property name="permissions">
        <element value="Add portal content"/>
      </property>
      <property name="visible">True</property>
    </object>
  </object>
</object>
