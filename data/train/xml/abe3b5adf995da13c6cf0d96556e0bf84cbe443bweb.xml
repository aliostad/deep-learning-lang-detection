<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
   PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
   "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

   <display-name>Generate Process</display-name>
   <description>Generate Process</description>

   <servlet>
      <servlet-name>CustomProcessServlet</servlet-name>
      <servlet-class>teamdash.process.CustomProcessServlet</servlet-class>
      <init-param>
         <param-name>prefix</param-name>
         <param-value>/Templates</param-value>
      </init-param>
   </servlet>

   <servlet-mapping>
      <servlet-name>CustomProcessServlet</servlet-name>
      <url-pattern>/teamdash.process.CustomProcessServlet</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>CustomProcessServlet</servlet-name>
      <url-pattern>/servlet/teamdash.process.CustomProcessServlet</url-pattern>
   </servlet-mapping>

</web-app>
