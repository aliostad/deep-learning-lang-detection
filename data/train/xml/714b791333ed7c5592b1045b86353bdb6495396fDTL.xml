<?xml version="1.0" encoding="UTF-8"?>
<codesense version="1.0">

    <set name="com.appsome.sugar.dtl.tags">
        <completion string=" for">
			<behavior>
				<append-dynamic>
					<matched-suffix>(\s*\{)|\s*[^{]</matched-suffix>
					<transform-into> ${1:item} in ${2:array} \%\}
	${3:content}
\{\% endfor \%\}
					</transform-into>
				</append-dynamic>
			</behavior>
		</completion>
		
		<completion string=" if">
			<behavior>
				<append-dynamic>
					<matched-suffix>(\s*\{)|\s*[^{]</matched-suffix>
					<transform-into> ${1:item} \%\}
	${2:content}
\{\% endif \%\}
					</transform-into>
				</append-dynamic>
			</behavior>
		</completion>
		
