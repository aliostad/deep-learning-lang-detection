    <set name="com.appsome.sugar.dtl.values">
    	<completion string=" ">
			<behavior>
				<append-dynamic>
					<matched-suffix>(\s*\{)|\s*[^{]</matched-suffix>
					<transform-into>${1:value} \}\}</transform-into>
